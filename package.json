{
  "name": "express-template",
  "version": "1.0.0",
  "repository": "ssh://git@github.com/Taiji-Kondo/express-template.git",
  "author": "Taiji-Kondo",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "run-p clean tsc",
    "clean": "rimraf .build",
    "dev": "ts-node-dev src/index.ts",
    "dev:test": "ts-node-dev --respawn src/index.ts",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint --fix . --ext .ts,.js",
    "tsc": "tsc"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
