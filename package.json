{
  "name": "express-template",
  "version": "1.0.0",
  "repository": "ssh://git@github.com/Taiji-Kondo/express-template.git",
  "author": "Taiji-Kondo",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production run-p clean tsc",
    "clean": "rimraf .build",
    "dev": "NODE_ENV=development ts-node-dev src/app.ts",
    "dev:test": "NODE_ENV=development ts-node-dev --respawn src/app.ts",
    "drop": "rimraf src/database/migrations",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint --fix . --ext .ts,.js",
    "migration:drop": "yarn drop && yarn typeorm schema:drop",
    "migration:generate": "yarn typeorm migration:generate -n default",
    "migration:run": "yarn typeorm migration:run",
    "prepare": "husky install",
    "tsc": "tsc",
    "typeorm": "ts-node ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.12",
    "@types/multer": "^1.4.5",
    "@types/mysql": "^2.15.18",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typeorm": "^0.2.34",
    "typeorm-routing-controllers-extensions": "^0.2.0",
    "typescript": "^4.3.2"
  }
}
